                    <div class="menu-item">
                      <a class="item">Logout</a>
                    </div>
                  </div>
                </div>
                <div class="col-md-10 edit-form">
                  <center>
                  <div class="form-group">
                  <%= form_for (@reviews) do |f| %>
                    <%= f.label :waitTime, id:"label1"%>
                      <%= f.text_field :waitTime,class: 'form-control' %>
                    </div>
                    <div class="form-group">
                        <%= f.label :besideManner,id:"label1" %>
                          <%= f.text_field :besideManner,class: 'form-control'  %>
                      </div>
                    <div class="form-group">
                        <%= f.label :overallRating,id:"label1" %>
                        <%= f.text_field :overallRating,class: 'form-control'  %>
                        <%= f.hidden_field :patient_id, :value => params[:patid]%>
                        <%= f.hidden_field :doctor_id, :value => params[:docid]%>
                      </div>
                      <div class="form-group">
                          <%= f.label :description,id:"label1" %>
                          <%= f.text_field :description,class: 'form-control'  %>
                      </div>
                            <div class="form-group">
                            <%= f.submit "Submit Review", class: "btn btn-edit" %>
                        </div>
                      <%end %>
