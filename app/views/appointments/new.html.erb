
<br/><br/><br/><br/><br/>
<%= form_for @appointment,:url =>{ :controller => :appointments, :action => :create, :docid =>@doctor.id , :patient =>@patient.id } do |f| %>
  <%= f.date_field :appdate%>

  <%= f.hidden_field :patient_id, :value => @patient.id%>
  <%= select("appointment", "appointtime",@slots.collect {|p| [p, p] }) %>
  <%= f.button :submit , :class=>"btn btn-edit" %>
<% end %>
